{
  "name": "bubbleup-plugin-test-mocha-istanbul",
  "version": "0.0.1",
  "description": "Istanbul plugin for the Mocha Bubbleup plugin",
  "main": "bubbleup-plugin-test-mocha-istanbul/index.js",
  "files": [
    "bubbleup-plugin-test-mocha-istanbul/index.js"
  ],
  "scripts": {
    "commit": "git-cz",
    "changelog": "conventional-changelog --infile CHANGELOG.md --same-file --release-count 0 --preset angular",
    "postchangelog": "git add CHANGELOG.md && git commit -m 'chore(CHANGELOG): append to changelog' && git push origin master",
    "test": "eslint bubbleup-plugin-test-mocha-istanbul/*.js",
    "release-patch": "npm run changelog && np patch",
    "release-minor": "npm run changelog && np minor",
    "release-major": "npm run changelog && np major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TylorS/bubble-plugin-test-mocha-istanbul.git"
  },
  "keywords": [
    "bubbleup-plugin",
    "bubbleup-plugin-test",
    "mocha",
    "istanbul",
    "reports",
    "es2015",
    "es6",
    "test",
    "runner"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com> (github.com/TylorS)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TylorS/bubble-plugin-test-mocha-istanbul/issues"
  },
  "homepage": "https://github.com/TylorS/bubble-plugin-test-mocha-istanbul#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "node ./node_modules/.bin/validate-commit-msg"
    }
  },
  "devDependencies": {
    "commitizen": "^2.8.5",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^1.1.7",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "ghooks": "^1.3.2",
    "np": "^2.8.0",
    "validate-commit-msg": "^2.6.1"
  },
  "dependencies": {
    "istanbul": "^1.1.0-alpha.1",
    "mocha-istanbul": "^0.3.0"
  },
  "peerDependencies": {
    "bubbleup-plugin-test-mocha": "^0.0.2"
  }
}
